<div class="page-header">
  <h3>Routine #<%= @routine.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Diagnosis</dt>
      <dd><%= @routine.diagnosis_id %></dd>

      <dt>Week</dt>
      <dd><%= @routine.week %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/routines" class="btn btn-primary">
        Back
      </a>
      <a href="/routines/<%= @routine.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_routine/<%= @routine.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
